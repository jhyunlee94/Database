-- 고양이와 개는 몇 마리 있을까
SELECT ANIMAL_TYPE, COUNT(ANIMAL_TYPE) AS "count"
    FROM ANIMAL_INS
    GROUP BY ANIMAL_TYPE
    ORDER BY ANIMAL_TYPE;
   
-- 동명 동물 수 찾기
GROUP BY의 조건은 HAVING 으로

SELECT NAME, COUNT(NAME) AS "COUNT"
    FROM ANIMAL_INS
    WHERE NAME IS NOT NULL
    GROUP BY NAME
    HAVING COUNT(NAME) > 1
    ORDER BY NAME ASC;
